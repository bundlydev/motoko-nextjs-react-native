{
  "name": "motoko-nextjs-react-native",
  "version": "0.1.0",
  "description": "ICP starter project with Motoko, Next.js and React Native",
  "author": "Bundly",
  "license": "MIT",
  "private": "true",
  "homepage": "https://github.com/bundlydev/motoko-nextjs-react-native#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bundlydev/motoko-nextjs-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/bundlydev/motoko-nextjs-react-native/issues"
  },
  "engines": {
    "node": ">=20.x"
  },
  "workspaces": [
    "frontend/*",
    "scripts/*"
  ],
  "scripts": {
    "dev": "turbo run dev --no-cache",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "generate": "dfx generate",
    "tunnel:start": "npx tsx ./scripts/tunnel/src/index.ts",
    "clean": "turbo run clean && rm -rf node_modules && rm -rf .turbo && rm -rf src &&  rm -rf .dfx"
  },
  "dependencies": {},
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2"
  }
}
